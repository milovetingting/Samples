<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wangyz.jetpack">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".lifecycle.LifeCycleActivity" />
        <activity android:name=".livedata.LiveDataActivity" />
        <activity android:name=".databinding.DataBindingActivity" />
        <activity android:name=".room.RoomActivity" />
        <activity android:name=".navigation.NavigationActivity" />
        <activity android:name=".paging.PagingActivity" />
        <activity android:name=".workmanager.WorkManagerActivity" />
    </application>

</manifest>